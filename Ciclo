import java.util.Scanner;
public class Ciclo{
    public static void main(String[] args){
        int in = 0;
        Scanner sc = new Scanner(System.in);
        in = sc.nextInt();
        long out = 1;
        if (in == 0){
            out = 1;
        }
        for (int i = 1; i <= in; i++){
            out = out * i;
        }
        System.out.println(out);
    }
}
